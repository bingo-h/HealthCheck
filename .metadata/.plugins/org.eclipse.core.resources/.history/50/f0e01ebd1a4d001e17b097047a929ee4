package com.bingo.server.mapper;

import java.util.List;

import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Select;

import com.bingo.server.po.Hospital;

@Mapper
public interface HospitalMapper {
	
	@Select("select * from hospitals where hId=#{hId}")
	public Hospital getHospitalsByHId(String hId);
	
	@Select("select * from hospitals where state=#{state}")
	public List<Hospital> getHospitalsByState(Integer state);
}
